        "obj-traverse": "^1.0.0",
        "pluralize": "^7.0.0",
        "resolve-url-loader": "^2.3.2",
        "sass": "^1.17.2",
        "sass-loader": "^7.1.0",
        "uuid": "^3.3.2",
        "vue": "^2.6.7",
        "vue-nestable": "^2.1.1",
        "vue-template-compiler": "^2.6.7"
    }
